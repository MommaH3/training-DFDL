<testSuite suiteName="NameDOB"
  xmlns:xs="http://www.w3.org/2001/XMLSchema" 
  xmlns:fn="http://www.w3.org/2005/xpath-functions"
  xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/" 
  xmlns:tdml="http://www.ibm.com/xmlns/dfdl/testData"
  xmlns:ex="http://example.com"
  xmlns:tns="http://example.com"
  defaultRoundTrip="onePass">

  <tdml:parserTestCase name="nameDOB_test1" root="file"
    model="lab1/com/tresys/csv/xsd/nameDOB1.dfdl.xsd">
    <tdml:document>
      <tdml:documentPart type="file">data/nameDOB1.csv</tdml:documentPart>
    </tdml:document>
    <tdml:infoset>
      <tdml:dfdlInfoset type="file">infosets/nameDOB1.xml</tdml:dfdlInfoset>
    </tdml:infoset>
  </tdml:parserTestCase>

  <tdml:parserTestCase name="nameDOB_test_bad_1" root="file"
                       model="lab1/com/tresys/csv/xsd/nameDOB1.dfdl.xsd">
    <tdml:document>
      <tdml:documentPart type="file">data/nameDOB1.bad.csv</tdml:documentPart>
    </tdml:document>
    <tdml:errors>
      <tdml:error>left over data</tdml:error>
    </tdml:errors>
  </tdml:parserTestCase>
  
</testSuite>
