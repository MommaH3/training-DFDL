<testSuite suiteName="NameDOB"
  xmlns:xs="http://www.w3.org/2001/XMLSchema" 
  xmlns:fn="http://www.w3.org/2005/xpath-functions"
  xmlns:dfdl="http://www.ogf.org/dfdl/dfdl-1.0/" 
  xmlns:tdml="http://www.ibm.com/xmlns/dfdl/testData"
  xmlns:ex="http://example.com"
  xmlns:tns="http://example.com"
  defaultRoundTrip="onePass">

  <tdml:parserTestCase name="nameDOB3_test1" root="file"
    model="lab3/com/tresys/csv/xsd/nameDOB3.dfdl.xsd" roundTrip="twoPass">
    <tdml:document>
      <tdml:documentPart type="file">data/nameDOB3.csv</tdml:documentPart>
    </tdml:document>
    <tdml:infoset>
      <tdml:dfdlInfoset type="file">infosets/nameDOB3.xml</tdml:dfdlInfoset>
    </tdml:infoset>
  </tdml:parserTestCase>

  <tdml:parserTestCase name="nameDOB3_test2" root="file"
                       model="lab3/com/tresys/csv/xsd/nameDOB3.dfdl.xsd"
                       roundTrip="twoPass">
    <!--
    Here's the data embedded directly in the test
    -->
    <tdml:document><![CDATA[last,first,middle,DOB
    smith|robert|brandon//03/24/1988
]]></tdml:document>
    <!--
    Here's the expected infoset also embedded in the test
    -->
    <tdml:infoset>
      <tdml:dfdlInfoset>
        <ex:file>
          <record>
            <lastName>smith</lastName>
            <firstName>robert</firstName>
            <middleName>brandon</middleName>
            <DOB>1988-03-24</DOB>
          </record>
        </ex:file>
      </tdml:dfdlInfoset>
    </tdml:infoset>
  </tdml:parserTestCase>

  <!--
  EXERCISE

  Write another parserTestCase that tests another case of data this time covering
  the corner case where the data has the title/heading line, but no rows of data
  at all.

  Will you need onePass or twoPass?

  This is partly created for you below.
  -->
